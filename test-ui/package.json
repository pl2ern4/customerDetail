{
  "name": "test-ui",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^1.4.1",
    "babel-preset-react": "^6.24.1",
    "material-ui": "^0.20.1",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-scripts": "1.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "jest",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "jest": "^23.4.2"
  },
  "jest": {
    "verbose": true,
    "testURL": "http://localhost/",
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "testRegex": "./__test__/.*.js$",
    "presets": [
      "env",
      "react"
    ],
    "plugins": [
      "transform-class-properties"
    ],
    "collectCoverageFrom": [
      "!**/node_modules/**"
    ]
  }
}
